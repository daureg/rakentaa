{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
        "include": ["./scripts"],
        "includePattern": ".+\\.js(doc)?$"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": false
        }
    },
    "opts": {
        "destination": "./doc/",
        "recurse": true,
        "private": true,
        "readme": "./README.md"
    }
}
